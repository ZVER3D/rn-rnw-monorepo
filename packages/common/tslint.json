{
  "extends": ["tslint:latest", "tslint-react", "tslint-react-hooks", "tslint-config-prettier"],
  "rules": {
    "object-literal-sort-keys": false,
    "no-empty-interface": false,
    "member-access": false,
    "jsx-no-lambda": false,
    "no-console": false,
    "jsx-boolean-value": false,
    "no-implicit-dependencies": false
  }
}
